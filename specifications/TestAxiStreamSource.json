{
    "vendor": "digilent.com",
    "ip_name": "TestAxiStreamSource",
    "underscore_name": "TEST_AXI_STREAM_SOURCE",
    "version": "1.0",
    "fpga_part": "xc7z020-clg400-1",
    "target_clk_period": "8.00",
    "axi4lite_interface": {
        "name": "S_AXI",
        "clock_domain": "s_axi_aclk",
        "reserved_addresses": 4,
        "reset": "s_axi_areset_n"

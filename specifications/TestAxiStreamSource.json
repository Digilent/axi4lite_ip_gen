            "name": "control",
            "access_type": "rw",
            "bitfields": [
                {
                    "name": "Start",
                    "high_bit": 0,
                    "low_bit": 0,
                    "clock_domain": "stream_clk"
                }
            ]
        },
        {
            "name": "status",
            "access_type": "ro",
            "bitfields": [
                {
                    "name": "Idle",
                    "high_bit": 0,
                    "low_bit": 0,
                    "clock_domain": "stream_clk"
                }
            ]
        }
    ]
}
{
    "vendor": "digilent.com",
    "ip_name": "Example",
    "version": "1.0",
    "axi4lite_interface": {
        "name": "S_AXI",
        "clock_domain": "s_axi_aclk",
        "reserved_addresses": 3,
        "reset": "s_axi_areset_n"
    },
    "clocks": [
        {
            "name": "reg_clk",
            "prefix": "r"
        },
        {
            "name": "s_axi_aclk",
            "prefix": "l"
        }
    ],
    "registers": [
        {
            "comment": ["all registers should be placed in ascending order of address in the register file and",
                        "start with base address [axi4lite_interface][reserved_addresses] * sizeof(u32)"],
            "name": "control",
            "bitfields": [
                {
                    "name": "Start",
                    "high_bit": 0,
                    "low_bit": 0,
                    "access_type": "rw",
                    "clock_domain": "reg_clk"
                },
                {
                    "name": "Idle",
                    "high_bit": 1,
                    "low_bit": 1,
                    "access_type": "ro",
                    "clock_domain": "reg_clk"
                }
            ]
        }
    ]
}